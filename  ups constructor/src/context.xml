<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="upsbean" class="com.spring.app.bean.UpsBean">
		<constructor-arg value="1234"></constructor-arg>
		<constructor-arg value="HH"></constructor-arg>
		<constructor-arg value="1000MV"></constructor-arg>
		<constructor-arg value="30000"></constructor-arg>
		<constructor-arg value="FALSE"></constructor-arg>
	</bean>
</beans>